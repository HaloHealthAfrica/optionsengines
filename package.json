{
  "name": "dual-engine-options-trading-platform",
  "version": "1.0.0",
  "description": "Production-ready options trading platform with dual decision engines",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "node build.js",
    "build:windows": "tsc && powershell -Command \"Copy-Item -Path 'src/migrations/*.sql' -Destination 'dist/migrations/' -Force\"",
    "start": "node dist/server.js",
    "start:server": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "tsx tests/e2e/cli.ts",
    "test:e2e:live": "node scripts/e2e-test.js",
    "test:e2e:engine-a": "tsx tests/e2e/cli.ts -- --engine-a",
    "test:e2e:engine-b": "tsx tests/e2e/cli.ts -- --engine-b",
    "test:e2e:ci": "tsx tests/e2e/cli.ts -- --ci",
    "test:e2e:nightly": "tsx tests/e2e/cli.ts -- --nightly",
    "migrate:up": "node dist/migrations/runner.js up",
    "migrate:down": "node dist/migrations/runner.js down",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "perf": "tsx scripts/performance/run-performance.ts",
    "check:webhooks": "tsx scripts/check-webhooks.ts",
    "test:webhook": "tsx scripts/test-webhook-endpoint.ts",
    "diagnose": "npm run check:webhooks && npm run test:webhook"
  },
  "keywords": [
    "options",
    "trading",
    "multi-agent",
    "a-b-testing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "ws": "^8.19.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/nock": "^10.0.3",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "nock": "^14.0.10",
    "prettier": "^3.1.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
